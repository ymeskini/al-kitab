{
  "name": "@al-kitab/api",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "dev": "nodemon ./src/index.ts --exec \"ts-node\" ./src/index.ts -e ts,graphql",
    "build": "tsc",
    "start": "node public/index.js",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "helmet": "3.21.2",
    "mongodb": "3.5.6",
    "mongoose": "5.9.10",
    "morgan": "1.9.1",
    "nodemon": "2.0.1",
    "ts-node": "8.5.4"
  },
  "devDependencies": {
    "@types/compression": "1.0.1",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/helmet": "0.0.45",
    "@types/mongodb": "3.5.14",
    "@types/mongoose": "5.7.14",
    "@types/morgan": "1.7.37",
    "@types/node": "12.12.15"
  }
}
